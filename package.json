{
    "name": "bannerlord-helper",
    "description": "A utility for NodeJS Cli for Mount & Blade II: Bannerlord",
    "version": "0.1.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "bin": {
        "bh": "./dist/bannerlord-helper.js"
    },
    "scripts": {
        "dev": "ts-node --transpile-only --esm bin/bannerlord-helper.ts",
        "lint": "xo src/**/*.ts --fix",
        "pretest": "npm run lint",
        "watch": "tsup --watch",
        "build": "npm run clean && tsup --dts --env.NODE_ENV production",
        "clean": "rimraf dist",
        "locale": "localize src/locale/zh-CN.ts -t en-US",
        "prepare": "husky"
    },
    "dependencies": {
        "@inquirer/prompts": "^6.0.1",
        "@kabeep/exception": "^1.2.3",
        "@kabeep/node-steam-library": "^0.1.0",
        "@kabeep/node-translate": "^1.1.6",
        "chalk": "^5.3.0",
        "cheerio": "^1.0.0",
        "compare-versions": "^6.1.1",
        "dedent": "^1.5.3",
        "fast-xml-parser": "^4.5.0",
        "got": "^13.0.0",
        "iconv-lite": "^0.6.3",
        "lodash.chunk": "^4.2.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.get": "^4.4.2",
        "lodash.merge": "^4.6.2",
        "lodash.partialright": "^4.2.1",
        "lodash.set": "^4.3.2",
        "lodash.snakecase": "^4.1.1",
        "lodash.uniqby": "^4.7.0",
        "lodash.words": "^4.2.0",
        "marked": "^14.1.2",
        "marked-terminal": "^7.1.0",
        "microsoft-translate-api": "^1.1.0",
        "node-html-markdown": "^1.3.0",
        "open": "^10.1.0",
        "ora": "^8.1.0",
        "os-locale": "^6.0.2",
        "sha1": "^1.1.1",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/got": "^9.6.12",
        "@types/lodash.chunk": "^4.2.9",
        "@types/lodash.clonedeep": "^4.5.9",
        "@types/lodash.get": "^4.4.9",
        "@types/lodash.merge": "^4.6.9",
        "@types/lodash.partialright": "^4.2.9",
        "@types/lodash.set": "^4.3.9",
        "@types/lodash.snakecase": "^4.1.9",
        "@types/lodash.uniqby": "^4.7.9",
        "@types/lodash.words": "^4.2.9",
        "@types/marked-terminal": "^6.1.1",
        "@types/node": "^22.5.4",
        "@types/sha1": "^1.1.5",
        "@types/yargs": "^17.0.33",
        "husky": "^9.1.5",
        "rimraf": "^5.0.10",
        "ts-node": "^10.9.2",
        "tsup": "^8.2.4",
        "typescript": "^5.6.2",
        "xo": "^0.58.0"
    },
    "author": "marowaki",
    "homepage": "https://github.com/marowaki/bannerlord-helper#readme",
    "repository": "git@github.com:marowaki/bannerlord-helper.git",
    "bugs": "https://github.com/marowaki/bannerlord-helper/issue",
    "engines": {
        "node": ">=18"
    },
    "license": "MIT"
}
